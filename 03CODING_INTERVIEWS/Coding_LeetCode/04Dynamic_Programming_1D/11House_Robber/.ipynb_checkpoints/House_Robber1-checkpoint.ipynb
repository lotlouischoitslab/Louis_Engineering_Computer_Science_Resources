    "### https://leetcode.com/problems/house-robber/"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "35891570",
   "metadata": {},
   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def rob(self, nums: List[int]) -> int:\n",
    "        rob1,rob2 = 0,0\n",
    "        for n in nums:\n",
    "            temp = max(n+rob1,rob2)\n",
    "            rob1 = rob2\n",
    "            rob2 = temp\n",
    "        return rob2"
   ]
  }
